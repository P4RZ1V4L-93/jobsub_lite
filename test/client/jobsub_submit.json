{
  "# example --json-config input file": "",
  "# used as input for jobsub_submit ": "",
  "# every line in this file is a dict entry": "",
  "# dict keys are jobsub_submit_flags":"",
  "# dict values get passed on to jobsub_submit as the value of the flag":"",
  "# except for dict keys that begin with '#' ":"",
  "# and are used as comments ":"",
  "# to turn off output of jobsub_submit flags ": "",
  "# such as --version, --help, etc  do the following": "",
  "--version": "False",
  "--help": "False",
  "--jobid-output-only": "False",
  "# or simply omit or comment out --help, etc from the json input file": "",
  "# from the json input file": "",
  "#                                                  ": "",    
  "# to turn on  --debug or other flags  do the following": "",
  "--debug": "True",
  "#                                                  ": "",    
  "# all other flags expect values which will be passed on ": "",
  "# to jobsub_submit": "",
  "# values can be strings or lists":"",
  "# if a flag is used only once in jobsub_submit its value ":"",
  "# will be a string":"",
  "# examples":"",
  "--group": "nova",
  "--jobsub-server": "fermicloud042.fnal.gov",
  "-N": "10",
  "#":"         ",
  "#":"         ",
  "#":"         ",
  "#":"         ",
  "#":"flags such as  -c, -e, --environment, -l, and --lines ",
  "#":"can be repeated, to do this use lists  ",
  "#":"",
  "#":"an example of jobsub_submit --environment FOO=BAR --environment BAZ=BOING",
  "--environment": [ "FOO=BAR", "BAZ=BOING" ],
  "#":"",
  "#":"jobsub_submit -c False=!=True -c True=!=False -c False=?=False example",
  "-c": [ "False=!=True",
          "True=!=False",
          "False=?=False" ],
  "-l": [ "+foo=\\\"bar\\\"", "+baz=\\\"boing\\\"" ] ,
  "--lines": [ "+foop=\\\"barp\\\"", "+bazp=\\\"boingp\\\"" ] ,
  "# lastly the job executable can be passed in this way": "",
  "# if it was not specified at the command line": "",
  "file:///home/dbox/nova_sleep.sh": "30",
  "#file:///home/dbox/nova_sleep.sh": [ "param1_for_nova_sleep.sh", 
                                       "param2",
                                       "param3", "etc" ],
  "# " : ""
}
